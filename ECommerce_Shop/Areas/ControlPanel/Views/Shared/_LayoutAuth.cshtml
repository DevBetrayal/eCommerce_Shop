
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" type="image/png" href="/Images/Frontend/@StaticValues.feviconURL" />
    <title>@ViewBag.Title</title>

    <link href="~/Content/Backend/bootstrap.min.css" rel="stylesheet">
    <link href="~/Fonts/Backend/font-awesome/css/font-awesome.css" rel="stylesheet">

    <link href="~/Content/Backend/animate.css" rel="stylesheet">
    <link href="~/Content/Backend/style.css" rel="stylesheet">

</head>
<body class="gray-bg">
    @RenderBody();

    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)

</body>
</html>
